<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.HelloController">
   <children>
      <Button fx:id="login" mnemonicParsing="false" onAction="#gotoLogin" prefHeight="50.0" prefWidth="120.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="register" mnemonicParsing="false" onAction="#gotoRegister" prefHeight="50.0" prefWidth="120.0" text="Register">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</VBox>
