<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo1.AdminHead"
      alignment="CENTER" prefHeight="199.0" prefWidth="1920.0">
<VBox.margin>
    <Insets bottom="50.0" />
</VBox.margin>
<opaqueInsets>
    <Insets />
</opaqueInsets>
<children>
    <Button fx:id="addBtn" mnemonicParsing="false" onAction="#gotoAddProduct" text="Add Product">
        <font>
            <Font size="24.0" />
        </font>
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </HBox.margin>
    </Button>
    <Button fx:id="orderBtn" mnemonicParsing="false" onAction="#gotoOrders" text="Pending Orders">
        <font>
            <Font size="24.0" />
        </font>
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </HBox.margin>
    </Button>
    <Button fx:id="logoutBtn" mnemonicParsing="false" onAction="#logout" text="Logout" style="-fx-background-color: #ff4d4d; -fx-text-fill: white;">
        <font>
            <Font size="24.0" />
        </font>
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </HBox.margin>
    </Button>
</children>
</HBox>